<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<!-- 拣货发单 -->
	<bean id="odata_OutstockMAction" class="com.sealinkin.odata.action.Odata_OutstockMAction" scope="prototype">
		<property name="odata_OutstockMImpl" ref="odata_OutstockMImpl"/>
	</bean>
	
	<!-- 出货调度 -->
	<bean id="odata_LocateAction" class="com.sealinkin.odata.action.Odata_LocateAction" scope="prototype">
		<property name="odata_LocateImpl" ref="odata_LocateImpl"/>
	</bean>
	
	<!-- 拣货回单 -->
	<bean id="odata_OutstockMReceiptAction" class="com.sealinkin.odata.action.Odata_OutstockMReceiptAction" scope="prototype">
		<property name="odata_OutstockMReceiptImpl" ref="odata_OutstockMReceiptImpl"/>
	</bean>
	
	<!-- 分播回单 -->
	<bean id="odata_DivideAction" class="com.sealinkin.odata.action.Odata_DivideAction" scope="prototype">
		<property name="odata_DivideImpl" ref="odata_DivideImpl"/>
	</bean>
	
	<!-- 出货手建单 -->
	<bean id="odata_ExpAction" class="com.sealinkin.odata.action.Odata_ExpAction" scope="prototype">
		<property name="odata_ExpImpl" ref="odata_ExpImpl"/>
	</bean>
	
	<!-- 出货装车作业 -->
	<bean id="odata_CarPlanAction" class="com.sealinkin.odata.action.Odata_CarPlanAction" scope="prototype">
		<property name="odata_CarPlanImpl" ref="odata_CarPlanImpl"/>
	</bean>
	<!-- 出货装车作业(小嘴) -->
	<bean id="odata_CarPlanForXzAction" class="com.sealinkin.odata.action.Odata_CarPlanForXzAction" scope="prototype">
		<property name="odata_CarPlanForXzImpl" ref="odata_CarPlanForXzImpl"/>
	</bean>
	
	<!-- 出货策略配置 -->
	<bean id="odata_TacticsAction" class="com.sealinkin.odata.action.Odata_TacticsAction" scope="prototype">
		<property name="odata_TacticsImpl" ref="odata_TacticsImpl"/>
	</bean>
		
	<!-- BYM出货订单接口-->
	<bean id="odata_BYMSheetAction" class="com.sealinkin.odata.action.Odata_BYMSheetAction" scope="prototype">
		<property name="odata_BYMSheetImpl" ref="odata_BYMSheetImpl"/>
	</bean>
	
	<!-- 切单规则配置 -->
	<bean id="oset_TaskAllotAction" class="com.sealinkin.odata.action.Oset_TaskAllotAction" scope="prototype">
		<property name="oset_TaskAllotImpl" ref="oset_TaskAllotImpl"/>
	</bean>
	
	<!-- 出货装车作业（天天惠） -->
	<bean id="odata_CarPlanForTthAction" class="com.sealinkin.odata.action.Odata_CarPlanForTthAction" scope="prototype">
		<property name="odata_CarPlanForTthImpl" ref="odata_CarPlanForTthImpl"/>
	</bean>

	<!-- 出货整理打包 -->
	<bean id="odata_ArrangePackAction" class="com.sealinkin.odata.action.Odata_ArrangePackAction" scope="prototype">
		<property name="odata_ArrangeImpl" ref="odata_ArrangeImpl"/>
	</bean>	
	
	<!-- 波次规则计划 -->
	<bean id="odata_WmsWavePlanAction" class="com.sealinkin.odata.action.Odata_WmsWavePlanAction" scope="prototype">
		<property name="odata_WmsWavePlanImpl" ref="odata_WmsWavePlanImpl"/>
	</bean>	
	
	<!-- 出货整理打包（电商）-->
	<bean id="odata_CheckPackOnlineAction" class="com.sealinkin.odata.action.Odata_CheckPackOnlineAction" scope="prototype">
		<property name="odata_CheckPackOnlineImpl" ref="odata_CheckPackOnlineImpl"/>
	</bean>	
	
	<!-- 病单处理 -->
	<bean id="odata_ExpCancelAction" class="com.sealinkin.odata.action.Odata_ExpCancelAction" scope="prototype">
		<property name="odata_ExpCancelImpl" ref="odata_ExpCancelImpl"/>
	</bean>
	
	<!-- 分播墙拣货 -->
	<bean id="odata_DivideWallAction" class="com.sealinkin.odata.action.Odata_DivideWallAction" scope="prototype">
