	<bean id="odata_CustSendDateAction" class="com.sealinkin.odata.action.Odata_CustSendDateAction" scope="prototype">
		<property name="odata_CustSendDateImpl" ref="odata_CustSendDateImpl"/>
	</bean>	
	
	<!-- 出车回单 -->
	<bean id="odata_CarReceiptAction" class="com.sealinkin.odata.action.Odata_CarReceiptAction" scope="prototype">
		<property name="odata_CarReceiptImpl" ref="odata_CarReceiptImpl"/>
	</bean>	
	<!-- 流水标签回单 -->
	<bean id="odata_OutstockMWaterAction" class="com.sealinkin.odata.action.Odata_OutstockMWaterAction" scope="prototype">
		<property name="odata_OutstockMWaterImpl" ref="odata_OutstockMWaterImpl"/>
	</bean>	
	<!-- 出库扫描 -->
	<bean id="odata_ExpCheckAction" class="com.sealinkin.odata.action.Odata_ExpCheckAction" scope="prototype">
		<property name="odata_ExpCheckImpl" ref="odata_ExpCheckImpl"/>
	</bean>	
		<!-- 拆零复核打包 -->
	<bean id="odata_BArrangePackAction" class="com.sealinkin.odata.action.Odata_BArrangePackAction" scope="prototype">
		<property name="odata_BArrangeImpl" ref="odata_BArrangeImpl"/>
	</bean>	
	
		<!--自动出货调度 -->
	<bean id="odata_LocateAutoAction" class="com.sealinkin.odata.action.Odata_LocateAutoAction" scope="prototype">
		<property name="odata_LocateAutoServiceImpl" ref="odata_LocateAutoServiceImpl"/>
	</bean>	
	
	<!--集货作业-->
	<bean id="odata_PackageAction" class="com.sealinkin.odata.action.Odata_PackageAction" scope="prototype">
		<property name="odata_PackageServiceImpl" ref="odata_PackageServiceImpl"/>
	</bean>	
	
	<!-- 出货定位策略配置 -->
	<bean id="Odata_outLocateStrategyAction" class="com.sealinkin.odata.action.Odata_outLocateStrategyAction" scope="prototype">
		<property name="Odata_outLocateStrategyImpl" ref="Odata_outLocateStrategyImpl"/>
	</bean>
	
	<!-- 复核策略配置 -->
	<bean id="Odata_CheckStrategyAction" class="com.sealinkin.odata.action.Odata_CheckStrategyAction" scope="prototype">
		<property name="Odata_CheckStrategyImpl" ref="Odata_CheckStrategyImpl"/>
	</bean>

	<!-- 接收订单数据接口配置 -->
	<bean id="odata_ReceiveSeverAction" class="com.sealinkin.odata.action.Odata_ReceiveSeverAction" scope="prototype">
		<property name="odata_ReceiveSeverImpl" ref="odata_ReceiveSeverImpl"/>
	</bean>
	<!-- 接收订单数据接口配置 -->
	<bean id="odata_ReceiveSeverImpl" class="com.sealinkin.odata.service.impl.Odata_ReceiveSeverImpl">
		<property name="genDao" ref="genDao" />
	</bean>
</beans>

