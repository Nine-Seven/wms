	</bean>		
	<!-- 病单审核 -->
	<bean id="odata_ExpCancelCheckAction" class="com.sealinkin.odata.action.Odata_ExpCancelCheckAction" scope="prototype">
		<property name="odata_ExpCancelCheckImpl" ref="odata_ExpCancelCheckImpl"/>
	</bean>	
	
