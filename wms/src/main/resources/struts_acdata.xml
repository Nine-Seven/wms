<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="acdata" extends="struts-default" namespace="/">
	<!-- 入库-->
		<action name="acdata_InStockAction_*" method="{1}" class="acdata_InStockAction"></action>
		<!-- 出库-->
		<action name="acdata_OutStockAction_*" method="{1}" class="acdata_OutStockAction"></action>
		
		<!-- 订单管理-->
		<action name="acdata_OrderAction_*" method="{1}" class="acdata_OrderAction"></action>
		
	</package>
</struts>
