<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="stock" extends="struts-default" namespace="/">
	<!--  虚拟库存调整-->
		<action name="stock_AdjustAccountsAction_*" method="{1}" class="stock_AdjustAccountsAction"></action>
	<!-- 库存调账手建单-->
		<action name="stock_PlanImportAction_*" method="{1}" class="stock_PlanImportAction"></action>
    <!-- 库存调账确认-->
		<action name="stock_ConfirmAction_*" method="{1}" class="stock_ConfirmAction"></action>
	
	</package>
</struts>
