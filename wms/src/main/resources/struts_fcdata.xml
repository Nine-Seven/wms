<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="fcdata" extends="struts-default" namespace="/">
		<!-- 盘点手建单 -->
		<action name="fcdata_PlanAction_*" method="{1}" class="fcdata_PlanAction"></action>
		
		<!-- 盘点回单 -->
		<action name="fcdata_RequestAction_*" method="{1}" class="fcdata_RequestAction"></action>
		
		<!-- 盘点发单 -->
		<action name="fcdata_CheckAction_*" method="{1}" class="fcdata_CheckAction"></action>
	</package>
</struts>
